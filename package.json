{
  "name": "mars2d-react-template",
  "version": "3.2.0",
  "description": "Mars2D平台,Vue最简项目模版",
  "scripts": {
    "dev": "vite",
    "serve": "npm run clean-cache && vite",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "preview": "vite preview",
    "build": "npm run lint && vite build",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix"
  },
  "dependencies": {
    "mars2d": "~3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.1",
    "@types/node": "^17.0.13",
    "@vitejs/plugin-react": "^1.0.7",
    "fs-extra": "^10.0.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "serve-static": "^1.14.2", 
    "vite": "^2.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars2d-vue-template.git"
  },
  "bugs": {
    "url": "https://github.com/marsgis/mars2d-vue-template/issues",
    "email": "wh@marsgis.cn"
  },
  "keywords": [
    "marsgis",
    "mars2d",
    "leaflet",
    "react",
    "gis",
    "webgis"
  ],
  "author": "火星科技",
  "license": "Apache-2.0",
  "homepage": "http://mars2d.cn"
}
